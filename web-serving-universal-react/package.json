{
  "name": "universal-create-react-app",
  "version": "0.1.0",
  "scripts": {
    "deploy-prod": "claudia update --handler index.handler --region us-east-1 --deploy-proxy-api --set-env-from-json prod-env.json --version production --keep --timeout 3",
    "deploy-dev": "claudia update --handler index.handler --region us-east-1 --deploy-proxy-api --set-env-from-json dev-env.json --version development --keep --timeout 3",
    "start": "node scripts/start.js",
    "nodemon": "nodemon --watch build/server build/server/bundle.js build/server/bundle.js",
    "build-client": "node scripts/build-client.js",
    "build-server": "node scripts/build-server.js",
    "build": "npm run build-client && npm run build-server",
    "test": "node scripts/test.js --env=jsdom",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "aws-serverless-express": "^3.0.2",
    "chalk": "1.1.3",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "fs-extra": "3.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-context-component": "^0.0.3",
    "react-dom": "^15.5.4",
    "react-router-dom": "^4.1.1",
    "webpack": "2.6.1",
    "autoprefixer": "7.1.0",
    "babel-cli": "^6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.0",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.0",
    "css-loader": "0.28.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.4",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "http-proxy-middleware": "^0.17.4",
    "isomorphic-fetch": "^2.2.1",
    "jest": "20.0.3",
    "nodemon": "^1.11.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "promise": "7.1.1",
    "react-dev-utils": "^11.0.4",
    "react-error-overlay": "^1.0.7",
    "style-loader": "0.17.0",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "2.0.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
