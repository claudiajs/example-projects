{
  "name": "web-api-postdeploy-config",
  "description": "An example of how to add post deploy variable configuration to web APIs",
  "private": true,
  "scripts": {
    "start": "claudia create --region us-east-1 --api-module web --custom-message",
    "configure": "claudia update --custom-message 'from-update'",
    "configure-interactive": "claudia update --custom-message",
    "deploy": "claudia update"
  },
  "devDependencies": {
    "claudia": "^1.4.1"
  },
  "dependencies": {
    "claudia-api-builder": "^1.4.1"
  }
}
